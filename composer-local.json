{
	"name": "greenpeace/planet4-slovakia",
	"description": "Greenpeace P4 slovakia",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-child-theme" : "0.*",
		"greenpeace/planet4-plugin-migrated-urls": "0.0.1",
		"wpackagist-plugin/wp-stateless": "2.1.7",
		"greenpeace/planet4-master-theme" : "dev-release/v1.65",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-develop",
		"greenpeace/planet4-plugin-gutenberg-engagingnetworks": "dev-master"
	},
	"scripts": {
		"core:language": "wp language core install sk_SK;wp language core activate sk_SK",
		"install:plugin-wpimport": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wp-all-import-pro_4.5.4.zip",
		"site:custom" : [
			"@core:language",
			"@install:plugin-wpimport"
		]
	}
}
